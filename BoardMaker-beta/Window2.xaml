            <Button BorderThickness="0" HorizontalAlignment="Left" Height="20" Margin="360,60,0,0" VerticalAlignment="Top" Width="20">
                <Button.Background>
                    <ImageBrush ImageSource="/заливка.png"/>
                </Button.Background>
            </Button>
            <Label Content="Статус :" Foreground="White" HorizontalAlignment="Left"  Height="34" Margin="653,7,0,0" VerticalAlignment="Top" Width="67" HorizontalContentAlignment="Center"/>
            <Border BorderBrush="Transparent" CornerRadius="6" BorderThickness="1" Background="White"  HorizontalAlignment="Left" Height="16" Margin="719,14,0,0" VerticalAlignment="Top" Width="138">

                <Label Content="Пробный период....." Foreground="Gray" HorizontalContentAlignment="Center" Width="104" FontSize="6" Height="24" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="-7,-2,0,0"></Label>

            </Border>
            <Button  BorderThickness="0" HorizontalAlignment="Left" Height="42" Margin="400,38,0,0" VerticalAlignment="Top" Width="40">
                <Button.Background>
                    <ImageBrush ImageSource="/кисть выбор.png" Stretch="Uniform"/>
