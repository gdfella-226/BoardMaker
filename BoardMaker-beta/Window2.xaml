            <Button  BorderThickness="0" HorizontalAlignment="Left" Height="42" Margin="460,38,0,0" VerticalAlignment="Top" Width="40">
                <Button.Background>
                    <ImageBrush Stretch="Fill" ImageSource="/дублировать.png"/>
                </Button.Background>
            </Button>
            <Button  BorderThickness="0" HorizontalAlignment="Left" Height="42" Margin="520,38,0,0" VerticalAlignment="Top" Width="40">
                <Button.Background>
                    <ImageBrush Stretch="Fill" ImageSource="/толщина.png"/>
                </Button.Background>
            </Button>





