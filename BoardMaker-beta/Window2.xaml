                    <ImageBrush ImageSource="/кисть.png"/>
                </Button.Background>
            </Button>
            <Button BorderThickness="0" HorizontalAlignment="Left" Height="22" Margin="340,38,0,0" VerticalAlignment="Top" Width="20" RenderTransformOrigin="1.021,0.58">
